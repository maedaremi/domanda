<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1><%= @user.name %></h1>
    </section>
    <section class="stats">
        <div class="user_avatars">
          <% @user.each do |user| %>
          <%= user.name %>
        </div>
      <% end %>
    </section>
  </aside>
  <div class="col-md-8">
    <h3><%= @title %></h3>
      <ul class="users follow">
        <%= render @user %>
      </ul>
  </div>
